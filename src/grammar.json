          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "package_spec": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_package_spec"
