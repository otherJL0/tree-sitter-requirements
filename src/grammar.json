{
  "name": "requirements",
  "rules": {
    "requirements": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version"
        },
        {
          "type": "SYMBOL",
          "name": "_version_specifier_clause"
        }
      ]
    },
    "_version_specifier_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version_specifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "version_specifier"
              }
            ]
          }
        }
      ]
    },
    "version_specifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_version_cmp"
        },
        {
          "type": "SYMBOL",
          "name": "version"
        }
      ]
    },
    "package_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "extras"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "extras": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "name"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "name_req": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "package_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_version_specifier_clause"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "_version_cmp": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "~="
        },
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": ">="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "==="
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Za-z\\d][\\w\\.\\-]*[A-Z\\d]"
    },
    "_pre_release": {
      "type": "PATTERN",
      "value": "\\.\\d+(a|b|rc)\\d+"
    },
    "_post_release": {
      "type": "PATTERN",
      "value": "\\.post\\d+"
    },
    "_developmental_release": {
      "type": "PATTERN",
      "value": "\\.dev\\d+"
    },
    "_local_version": {
      "type": "PATTERN",
      "value": "\\.\\d+\\+[\\d\\w]+"
    },
    "version": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\.\\d+"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_pre_release"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_post_release"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_developmental_release"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_local_version"
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "PATTERN",
                        "value": "\\.\\d+"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\.\\*"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

