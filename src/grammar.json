{
  "name": "requirements",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_package_expression"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "_package_expression": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_package"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_version_constraints"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "_package": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "extra"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "extra": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "_version_constraints": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "operators",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "=="
              },
              {
                "type": "STRING",
                "value": "!="
              },
              {
                "type": "STRING",
                "value": ">="
              },
              {
                "type": "STRING",
                "value": "<="
              },
              {
                "type": "STRING",
                "value": "~="
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "version"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Za-z]+[A-Za-z-_\\d]+"
    },
    "extras": {
      "type": "PATTERN",
      "value": "\\[.+\\]"
    },
    "version": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+(\\.\\d[A-Za-z]*\\d*)?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

