      "value": "[A-Za-z0-9][A-Za-z0-9\\.\\-_]*[A-Za-z0-9]"
    },
    "str": {
      "type": "PATTERN",
      "value": "(\\'[^\\']*\\'|\\\"[^\\\"]*\\\")"
    },
    "_pre_release": {
      "type": "PATTERN",
      "value": "\\.\\d+(a|b|rc)\\d+"
    },
    "_post_release": {
      "type": "PATTERN",
      "value": "\\.post\\d+"
    },
    "_developmental_release": {
      "type": "PATTERN",
      "value": "\\.dev\\d+"
    },
    "_local_version": {
      "type": "PATTERN",
      "value": "\\.\\d+\\+[\\d\\w]+"
    },
    "version": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\.\\d+"
          }
        },
        {
          "type": "CHOICE",
          "members": [
